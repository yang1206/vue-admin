{
  "name": "vue-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/vue-template.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "test": "vitest",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@tanstack/vue-query": "^4.29.5",
    "@unocss/reset": "0.51.13",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.4.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.36",
    "vue": "^3.3.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@iconify-json/carbon": "^1.1.16",
    "@iconify-json/line-md": "^1.1.24",
    "@types/node": "^18.16.8",
    "@types/nprogress": "^0.2.0",
    "@unocss/eslint-config": "^0.51.12",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/test-utils": "^2.3.2",
    "commitizen": "^4.3.0",
    "cz-git": "^1.6.1",
    "eslint": "8.41.0",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "typescript": "^5.0.4",
    "unocss": "0.51.13",
    "unplugin-auto-import": "0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "4.3.8",
    "vite-plugin-vue-devtools": "^0.0.22",
    "vitest": "^0.31.0",
    "vue-tsc": "1.6.5"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
